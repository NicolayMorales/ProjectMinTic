@page
@model ProyectoCiclo3.App.Frontend.Pages.EditAeropuertoModel
@{
   
 var aeropuerto = Model.Aeropuerto;
}
 
<div class="text-center">
    <form method="POST">
  <label for="nombre" class="form-label">Nombre:</label>
  
    <label for="ciudad" class="form-label">Ciudad:</label>
  
  <label for="pais" class="form-label">Pais:</label>
  
  <label for="coord_x" class="form-label">Coord_x:</label>
  <input type="number" class="form-control" id="coord_x" name="coord_x" placeholder="Coord_x"asp-for="@aeropuerto.coord_x">
  <label for="coord_y" class="form-label">Coord_y:</label>
  <input type="number" class="form-control" id="coord_y" name="coord_y" placeholder="Coord_y"asp-for="@aeropuerto.coord_y">
  <hr>
  <button type="submit" class="btn btn-success">Enviar</button>
</form>
</div>
